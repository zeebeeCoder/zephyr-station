{
  "name": "zephyr-frontend",
  "version": "0.1.0",
  "private": true,
  "pnpm": {
    "onlyBuiltDependencies": [
      "duckdb",
      "esbuild"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "seed": "tsx lib/db/seed.ts",
    "seed:fresh": "rm -f local.db && pnpm seed",

    "typecheck": "tsc --noEmit",
    "lint": "next lint"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.88",
    "@ai-sdk/react": "^2.0.118",
    "ai": "^5.0.116",
    "clsx": "^2.1.1",
    "duckdb-async": "^1.4.2",
    "next": "^16.1.0",
    "postgres": "^3.4.7",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
